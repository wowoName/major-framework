 QTabBar[shape="4"], QTabBar[shape="0"], QTabBar[shape="1"], QTabBar[shape="5"]
{
   font:57 16px "文泉驿正黑";
}


  QTabBar[shape="4"]::tab:selected,  QTabBar[shape="0"]::tab:selected,  QTabBar[shape="1"]::tab:selected,  QTabBar[shape="5"]::tab:selected
{
   color:#d4f1ff;
   background-color:#20617c;
