 QSlider::add-page::Horizontal
{
        background-color:#193f66;
        height:3px;
        border-radius:3px;
}

 QSlider::sub-page::Horizontal
{
        background-color:qlineargradient(spread:pad,x1:0,x2:1,y1:0,y2:0,stop:0 #76c5d3,stop:1 #34c390);
        height:3px;
        border-radius:3px;
}

/*整个划动条设置*/
 QSlider::groove:Horizontal
{
        height:8px;
        background:#193f66;
}

/*拖动设置*/
 QSlider::handle:Horizontal
{
width:16px;
border:1px solid #193f66;
border-radius:8px;
background-color:#d4f1ff;
margin:-5px 0px;/*设定圆圈*/
}

 QSlider::sub-page::Vertical
{
        background-color:qlineargradient(spread:pad,x1:0,x2:1,y1:0,y2:0,stop:0 #76c5d3,stop:1 #34c390);
        width:3px;
        border-radius:3px;
}

/*整个划动条设置*/
 QSlider::groove:Vertical
{
        width:8px;
        background:#193f66;
}

/*拖动设置*/
 QSlider::handle:Vertical
{
height:16px;
border:1px solid #193f66;
border-radius:8px;
background-color:#d4f1ff;
margin:0px -5px;/*设定圆圈*/
}


