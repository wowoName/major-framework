 QTabBar[shape="4"], QTabBar[shape="0"], QTabBar[shape="1"], QTabBar[shape="5"]
{
   font:57 16px "文泉驿正黑";
}


  QTabBar[shape="4"]::tab:selected,  QTabBar[shape="0"]::tab:selected,  QTabBar[shape="1"]::tab:selected,  QTabBar[shape="5"]::tab:selected
{
   color:#222222;
   background-color:#ffffff;
   height:25px;
   font:57 16px "文泉驿正黑";
   border:1px solid #ccd4db;
padding:3px 5px;
outline:none;
}

 QTabBar[shape="4"]::tab, QTabBar[shape="0"]::tab, QTabBar[shape="1"]::tab, QTabBar[shape="5"]::tab
{
outline:none;
}

 QTabBar[shape="4"]::tab:!selected, QTabBar[shape="0"]::tab:!selected, QTabBar[shape="1"]::tab:!selected, QTabBar[shape="5"]::tab:!selected
{
    background-color:#dfe7f0;
    color:#222222;
    font:57 16px "文泉驿正黑";
    height:25px;
   border:1px solid #ccd4db;
padding:3px 5px;
outline:none;
}



